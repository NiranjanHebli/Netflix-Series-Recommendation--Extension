{
    "manifest_version": 2,
    "name": "Random Series Recommendation",
    "version": "1.0",
    "description": "Selects random series from Netflix",
    "icons": {
      "128": "logo.png"
    },
    "permissions": [
      "tabs"
    ],
    "browser_action": {
      "default_icon": {
        "128": "logo.png"
      },
      "default_popup": "popup.html"
    }
  }